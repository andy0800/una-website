{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/ar/index.html"
    },
    {
      "source": "/ar",
      "destination": "/ar/index.html"
    },
    {
      "source": "/ar/(.*)",
      "destination": "/ar/$1.html"
    },
    {
      "source": "/en",
      "destination": "/en/index.html"
    },
    {
      "source": "/en/(.*)",
      "destination": "/en/$1.html"
    },
    {
      "source": "/admin",
      "destination": "/admin/login.html"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/admin/$1.html"
    }
  ]
}
