{
  "version": 2,
  "rewrites": [
    {
      "source": "/ar/:path*/",
      "destination": "/ar/:path*.html"
    },
    {
      "source": "/ar/:path*",
      "destination": "/ar/:path*.html"
    },
    {
      "source": "/en/:path*/",
      "destination": "/en/:path*.html"
    },
    {
      "source": "/en/:path*",
      "destination": "/en/:path*.html"
    },
    {
      "source": "/admin/:path*/",
      "destination": "/admin/:path*.html"
    },
    {
      "source": "/admin/:path*",
      "destination": "/admin/:path*.html"
    },
    {
      "source": "/",
      "destination": "/ar/index.html"
    }
  ]
}
