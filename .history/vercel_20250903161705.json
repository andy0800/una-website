{
  "version": 2,
  "builds": [
    {
      "src": "frontend/ar/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/ar/index.html"
    }
  ],
  "functions": {
    "frontend/ar/index.html": {
      "runtime": "@vercel/static"
    }
  },
  "outputDirectory": "frontend",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
