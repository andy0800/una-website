{
  "version": 2,
  "rewrites": [
    {
      "source": "/ar/:slug",
      "destination": "/ar/:slug.html"
    },
    {
      "source": "/en/:slug",
      "destination": "/en/:slug.html"
    },
    {
      "source": "/admin/:slug",
      "destination": "/admin/:slug.html"
    },
    {
      "source": "/",
      "destination": "/ar/index.html"
    }
  ]
}